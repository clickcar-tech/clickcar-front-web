<svg width="200" height="60" viewBox="0 0 200 60" fill="none" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="logoGradient" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#FF6906;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#FF6906;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Logo Icon (Key/Button shape) -->
  <g transform="translate(20, 20)">
    <!-- Main body of the key/button -->
    <rect x="0" y="8" width="32" height="16" rx="8" fill="url(#logoGradient)"/>
    <!-- Button circle -->
    <circle cx="24" cy="16" r="6" fill="white"/>
  </g>
  
  <!-- Text "CLICK CAR" -->
  <text x="70" y="40" fill="#000000" font-family="Montserrat, sans-serif" font-size="24" font-weight="bold" letter-spacing="2px">
    CLICK CAR
  </text>
</svg>